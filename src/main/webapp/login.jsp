<script type="text/javascript">
$("#d1").blur(function () {
	var val = $("#d1").val();
	$.ajax({
		url:"/SSM-MAVEN/user.do",
		type:"POST",
		data:{"username":val},
		success:function(mas){
			alert(mas.code+"---"+mas.msg)
			
		},
		error:function(){
			alert("服务器正在吃橘子")
		}
		
	});
